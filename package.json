{
  "name": "api-laravel-ts",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev src/server.ts",
    "test": "jest",
    "migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "test:isolated": "export DATABASE_URL=\"file:./test.db\" && npx prisma db push --accept-data-loss && jest"
  },
  "dependencies": {
    "@prisma/client": "5.10.0",
    "audit": "^0.0.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fix": "^0.0.3",
    "jsonwebtoken": "^9.0.1",
    "npm": "^11.6.0",
    "xlsx": "^0.18.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.1",
    "prisma": "5.10.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
